{
  "name": "amqp-log-to-pg",
  "description": "Logs from an amqp queue to a db.",
  "version": "1.2.0-update.0",
  "author": "Tim Allen <tim@noblesamurai.com>",
  "license": "BSD",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc -a -c -r html -r text npm test",
    "pretest": "xo",
    "test": "mocha --recursive test",
    "watch": "mocha --recursive --watch test",
    "docs": "jsdoc2md index.js",
    "start": "node index.js"
  },
  "homepage": "https://github.com/noblesamurai/amqp-log-to-pg",
  "repository": {
    "type": "git",
    "url": "git://github.com/noblesamurai/amqp-log-to-pg.git"
  },
  "bugs": {
    "url": "https://github.com/noblesamurai/amqp-log-to-pg/issues"
  },
  "engines": {
    "node": ">=10",
    "npm": "6.x"
  },
  "dependencies": {
    "amqp-wrapper": "^5.5.0",
    "debug": "^2.6.6",
    "knex": "^0.21.5",
    "pg": "^6.1.5"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "proxyquire": "^1.7.11",
    "sinon": "^9.0.3",
    "xo": "^0.33.1"
  },
  "keywords": [],
  "nyc": {
    "exclude": [
      "coverage",
      "test"
    ]
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "xo": {
    "env": "mocha",
    "extends": "semistandard"
  }
}
